{
    "name": "bdk/simplecache",
	"type": "library",
	"description": "SimpleCache implementation with adapters for APC(u), Couchbase, Filesystem, FlySystem, Memcached, Redis, and SQL   Stampede protection) built in",
    "keywords": [
        "cache", "caching",
        "key", "value", "store",
        "apc",
        "memcached",
        "redis",
        "couchbase",
        "sql", "mysql", "sqlite", "postgresql",
        "filesystem", "flysystem",
        "stampede", "protection",
        "psr-6", "psr-cache",
        "psr-16", "psr-simple-cache"
    ],
	"homepage": "http://github.com/bkdotcom/SimpleCache",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Mullie",
            "homepage": "https://www.mullie.eu",
            "email": "scrapbook@mullie.eu",
            "role": "Developer of scrapbook"
        },
		{
			"name": "Brad Kent",
			"homepage": "http://www.bradkent.com/",
			"email": "bkfake-packagist@yahoo.com",
			"role": "Developer"
		}

    ],
    "require": {
        "php": "^5.3|^7.0",
        "psr/cache": "~1.0",
        "psr/simple-cache": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|~5.0|~6.0",
        "friendsofphp/php-cs-fixer": "~2.0"
    },
    "suggest": {
        "ext-apc": ">=3.1.1",
        "ext-couchbase": ">=2.0.0",
        "ext-memcached": ">=2.0.0",
        "ext-redis": ">=2.2.0 || 0.0.0.0",
        "ext-pdo": ">=0.1.0",
        "league/flysystem": "~1.0"
    },
    "provide": {
        "psr/cache-implementation": "~1.0",
        "psr/simple-cache-implementation": "~1.0"
    },
    "autoload": {
        "psr-4": { "bdk\\SimpleCache\\": "src/" }
	},
	"autoload-dev": {
        "psr-4": { "bdk\\SimpleCache\\Tests\\": "tests/" }
	}
}
